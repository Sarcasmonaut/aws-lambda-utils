{
  "name": "aws-lambda-utils",
  "version": "0.1.1",
  "description": "Decorators and utilities for the average AWS Lambda development",
  "repository": "git@github.com:Sarcasmonaut/aws-lambda-utils.git",
  "author": "Sarcasmonaut <patrik.kessler@outlook.de>",
  "license": "MIT",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "prepack": "npm run build",
    "test": "jest --passWithNoTests --forceExit",
    "test:coverage": "npm test --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/aws-lambda": "^8.10.64",
    "@types/jest": "^26.0.3",
    "@types/lodash": "^4.14.165",
    "@types/node": "^10.12.18",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^26.5.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "lodash": "^4.17.20",
    "reflect-metadata": "^0.1.13"
  },
  "files": [
    "lib"
  ]
}
